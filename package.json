{
  "name": "tmux-team",
  "version": "4.0.0",
  "description": "CLI tool for AI agent collaboration in tmux - manage cross-pane communication",
  "type": "module",
  "bin": {
    "tmux-team": "./bin/tmux-team",
    "tmt": "./bin/tmux-team"
  },
  "scripts": {
    "dev": "tsx src/cli.ts",
    "tmt": "./bin/tmux-team",
    "test": "pnpm test:run",
    "test:watch": "vitest",
    "test:run": "vitest run --coverage && node scripts/check-coverage.mjs --threshold 90 --branches 85",
    "lint": "oxlint src/",
    "lint:fix": "oxlint src/ --fix",
    "format": "prettier --write src/",
    "format:check": "prettier --check src/",
    "type:check": "tsc --noEmit",
    "check": "pnpm type:check && pnpm lint && pnpm format:check"
  },
  "keywords": [
    "tmux",
    "cli",
    "ai",
    "agent",
    "collaboration",
    "multi-agent"
  ],
  "author": "Ben Hsieh",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wkh237/tmux-team.git"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "pnpm@9.15.4",
  "os": [
    "darwin",
    "linux"
  ],
  "files": [
    "bin",
    "src",
    "skills"
  ],
  "dependencies": {
    "tsx": "^4.7.0"
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "@vitest/coverage-v8": "^1.6.1",
    "oxlint": "^1.34.0",
    "prettier": "^3.7.4",
    "typescript": "^5.3.0",
    "vitest": "^1.2.0"
  }
}
